{
  "put": {
    "tags": [
      "{{ parent }}"
    ],
    {{consume_json}},
    {{produce_json}},
    "parameters": [
      {
        "in": "body",
        "name": "SingleStreamEvent",
        "required": true,
        "schema": {
          "$ref": "#/definitions/SingleStreamEvent"
        }
      }
    ],
    "responses": {
      {{Success_200}},
      {{Error_400}},
      {{Error_500}}
    },
    {{security_key_iam}},
    "x-amazon-apigateway-integration": {
      "credentials": "{{kinesis.credentials}}",
      {{inject_org_id_as_template}},
      "uri": "arn:aws:apigateway:us-east-1:kinesis:action/PutRecord",
      "passthroughBehavior": "never",
      "responses": {
        "200": {
          "statusCode": "200"
        },
        "400": {
          "statusCode": "400"
        },
        "500": {
          "statusCode": "500"
        }
      },
      "requestParameters": {
        "integration.request.header.Content-Type": "'application/x-amz-json-1.1'"
      },
      "httpMethod": "POST",
      "type": "aws"
    }
  }
}
