{
  "put": {
    "tags": [
      "{{ parent }}"
    ],
    {{consume_json}},
    {{produce_json}},
    "parameters": [
      {
        "in": "body",
        "name": "SingleStreamEvent",
        "required": true,
        "schema": {
          "$ref": "#/definitions/SingleStreamEvent"
        }
      }
    ],
    "responses": {
      {{Success_200}},
      {{Error_400}},
      {{Error_500}}
    },
    {{security_key_iam}},
    "x-amazon-apigateway-integration": {
      "credentials": "{{kinesis.credentials}}",
      "requestTemplates": {
        "application/json": "#set($orgID = $context.identity.apiKey)\n#set($bodyJson = $util.parseJson($input.body))\n#set($bodyJson.companykey = $orgID)\n{\n    \"StreamName\": \"{{kinesis.stream}}\",\n    \"Records\": [\n        #foreach($elem in $bodyJson)\n{\n            #set($elem.companykey = $orgID)\n            \"Data\": \"$util.base64Encode($elem)\",\n            \"PartitionKey\": \"$orgID\"\n        }#if($foreach.hasNext),#end\n        #end\n    ]\n}\n"
      },
      "uri": "arn:aws:apigateway:us-east-1:kinesis:action/PutRecord",
      "passthroughBehavior": "never",
      "responses": {
        "200": {
          "statusCode": "200"
        },
        "400": {
          "statusCode": "400"
        },
        "500": {
          "statusCode": "500"
        }
      },
      "requestParameters": {
        "integration.request.header.Content-Type": "'application/x-amz-json-1.1'"
      },
      "httpMethod": "POST",
      "type": "aws"
    }
  }
}
