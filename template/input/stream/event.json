{
  "put": {
    "tags": [
      "{{ parent }}"
    ],
    {{consume_json}},
    {{produce_json}},
    "parameters": [
      {
        "in": "body",
        "name": "SingleStreamEvent",
        "required": true,
        "schema": {
          "$ref": "#/definitions/SingleStreamEvent"
        }
      }
    ],
    "responses": {
      {{Success_200}},
      {{std_error_responses}}
    },
    {{security_key_iam}},
    "x-amazon-apigateway-integration": {
      "credentials": "{{api.stream.credentials}}",
      "requestTemplates": {
        "application/json": "#set($orgID = $context.identity.apiKey)\n{\n    \"companykey\": \"$orgID\",\n    \"event\": $input.json('$')\n}"
      },
      "uri": "arn:aws:apigateway:us-east-1:lambda:path/2015-03-31/functions/arn:aws:lambda:us-east-1:766732214526:function:{{lambda.IngestToRawLambda.function.name}}/invocations",
      "passthroughBehavior": "never",
      "responses": {
        "default": {
          "statusCode": "200"
        },
        {{std_error_mappings}}
      },
      "httpMethod": "POST",
      "type": "aws"
    }
  }
}
