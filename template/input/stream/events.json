{
  "put": {
    "tags": [
      "{{ parent }}"
    ],
    {{consume_json}},
    {{produce_json}},
    "parameters": [
      {
        "in": "body",
        "name": "MultiStreamEvent",
        "required": true,
        "schema": {
          "$ref": "#/definitions/MultiStreamEvent"
        }
      }
    ],
    "responses": {
      "200": {
        "statusCode": "200",
        "schema": {
          "$ref": "#/definitions/StreamEventsResponse"
        }
      },
      {{Error_400}},
      {{Error_500}}
    },
    "security": [
      {
        "sigv4": []
      },
      {
        "api_key": []
      }
    ],
    "x-amazon-apigateway-integration": {
      "credentials": "{{api.stream.kinesis.credentials}}",
      "requestTemplates": {
        "application/json": "#set($orgID = $context.identity.apiKey)\n#set($bodyJson = $util.parseJson($input.body))\n#set($bodyJson.companykey = $orgID)\n{\n    \"StreamName\": \"{{api.stream.kinesis.stream}}\",\n    \"Records\": [\n        #foreach($elem in $bodyJson)\n{\n            #set($elem.companykey = $orgID)\n            \"Data\": \"$util.base64Encode($elem)\",\n            \"PartitionKey\": \"$orgID\"\n        }#if($foreach.hasNext),#end\n        #end\n    ]\n}\n"
      },
      "uri": "arn:aws:apigateway:us-east-1:kinesis:action/PutRecords",
      "passthroughBehavior": "never",
      "responses": {
        "200": {
          "statusCode": "200",
          "responseTemplates": {
            "application/json": "#set($inputRoot = $input.path('$'))\n{ \n    \"failedRecordCount\": $inputRoot.FailedRecordCount,\n    \"records\" : $inputRoot.Records\n}"
          }
        },
        "4\\d{2}": {
          "statusCode": "400"
        },
        "5\\d{2}": {
          "statusCode": "500"
        }
      },
      "requestParameters": {
        "integration.request.header.Content-Type": "'application/x-amz-json-1.1'"
      },
      "httpMethod": "POST",
      "type": "aws"
    }
  }
}
