{
  //READ Tenant
  "put": {
    {{parent_tag}},
    {{consume_json}},
    {{produce_json}},
    "parameters": [
      {{user_request_parameter}}
    ],
    "responses": {
      {{Success_200}},
      {{std_error_responses}}
    },
    {{allow_origin_header}},
    // creates the API Key for the user/tenant, so we can't pass one in.
    {{security_key_iam-no-api-key}},
    "x-amazon-apigateway-integration": {
      {{meta_credentials}},
      "requestTemplates": {
        "application/json": "{\n  \"apiKey\" : \"$context.identity.apiKey\",\n  \"create\": $input.json('$')\n}"
      },
      {{tenant_crud_innvocation}},
      {{never_passthrough}},
      "responses": {
        {{default_200_response}},
        {{std_error_mappings}}
      },
      {{post_text_aws_integration}}
    }
  },
  //READ Tenant
  "get": {
    {{produce-consume-json_standard-responses_iam}},
    "x-amazon-apigateway-integration": {
      {{meta_credentials}},
      "requestTemplates": {
        "application/json": "{\n  \"apiKey\" : \"$context.identity.apiKey\",\n  \"read\" :{}\n    }"
      },
      {{tenant_crud_innvocation}},
      {{never_passthrough}},
      "responses": {
        {{default_200_response}},
        {{std_error_mappings}}
      },
      {{post_text_aws_integration}}
    }
  },
  // UPDATE Tenant
  "patch": {
      "tags": [
      "{{ parent }}"
    ],
    {{consume_json}},
    {{produce_json}},
    "parameters": [
      {
        "in": "body",
        "name": "UpdateTenant",
        "required": true,
        "schema": {
          "$ref": "#/definitions/UpdateTenant"
        }
      }
    ],
    "responses": {
      {{Success_200}},
      {{std_error_responses}}
    },
    {{security_key_iam}},
    "x-amazon-apigateway-integration": {
      {{meta_credentials}},
      "requestTemplates": {
        "application/json": "{\n    \"apiKey\" : \"$context.identity.apiKey\",\n\"update\": $input.json('$')\n}"
      },
      {{tenant_crud_innvocation}},
      {{never_passthrough}},
      "responses": {
        {{default_200_response}},
        {{std_error_mappings}}
      },
      {{post_text_aws_integration}}
    }
  },
// Only allow website access for GET (read) and Update
  {{cors_get-patch}}
}
