{
  //LIST all the keys for a device
  "get": {
    {{parent_tag}},
    {{consume_json}},
    {{produce_json}},
    "parameters": [
      {
        "name": "deviceId",
        "in": "path",
        "required": true,
        "type": "string"
      }
    ],
    "responses": {
      {{Success_200}},
      {{std_error_responses}}
    },
    {{security_key_iam}},
    "x-amazon-apigateway-integration": {
      {{meta_credentials}},
      "requestTemplates": {
        "application/json": "{\n  \"apiKey\" : \"$context.identity.apiKey\",  \"list\":{\"id\" : \"$input.params('deviceId')\",\n}\n}"
      },
      {{key-innvocation_mapping}}
  }
  },
  //CREATE a key
  "put": {
    {{parent_tag}},
    {{consume_json}},
    {{produce_json}},
    "parameters": [
      {
        "in": "body",
        "name": "DeviceInfo",
        "required": true,
        "schema": {
          "$ref": "#/definitions/DeviceInfoRequest"
        }
      }
    ],
    "responses": {
      {{Success_200}},
      {{std_error_responses}}
    },
    {{allow_origin_header}},
    {{security_key_iam}},
    "x-amazon-apigateway-integration": {
      {{meta_credentials}},
      "requestTemplates": {
        "application/json": "{\n  \"apiKey\" : \"$context.identity.apiKey\",  \n  \"create\" : $input.json('$')\n}"
      },
      {{key-innvocation_mapping}}
    }
  },
  // Delete a key
  "delete": {
    {{parent_tag}},
    {{consume_json}},
    {{produce_json}},
    // require both the device ID and the Key ID to delete
    "parameters": [
      {
        "in": "body",
        "name": "KeyInfo",
        "required": true,
        "schema": {
          "$ref": "#/definitions/KeyRequest"
        }
      }
    ],
    "responses": {
      {{Success_200}},
      {{std_error_responses}}
    },
    {{allow_origin_header}},
    {{security_key_iam}},
    "x-amazon-apigateway-integration": {
      {{meta_credentials}},
      "requestTemplates": {
        "application/json": "{\n  \"apiKey\" : \"$context.identity.apiKey\",  \n  \"delete\" : $input.json('$')\n}"
      },
      {{key-innvocation_mapping}}
    }
  },
// Allow website access to everything
{{cors_put-get-delete}}
}
