{
  "swagger": "2.0",
  "info": {
    "version": "{{ api.version }}",
    "title": "{{ api.title }}",
    "contact": {
      "name": "Fineo",
      "url": "http://fineo.io",
      "email": "help@fineo.io"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "https://www.apache.org/licenses/LICENSE-2.0"
    }
  },
  "host": "{{ api.host }}",
  "basePath": "/{{ api.version }}",
  "schemes": [
    "https"
  ],
  "paths": {
    {{ paths | join: "," }}
  },
  "securityDefinitions": {
    "api_key": {
      "type": "apiKey",
      "name": "x-api-key",
      "in": "header"
    },
    "Fineo-customers": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header",
      "x-amazon-apigateway-authtype": "cognito_user_pools",
      "x-amazon-apigateway-authorizer": {
        "providerARNs": [
          "arn:aws:cognito-idp:us-east-1:766732214526:userpool/us-east-1_B5WQVmvFb"
        ],
        "type": "cognito_user_pools"
      }
    },
    "sigv4": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header",
      "x-amazon-apigateway-authtype": "awsSigv4"
    }
  },
  "definitions": {
    {{ definitions | join: "    ," }}
  }
}
