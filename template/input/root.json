{
  "swagger": "2.0",
  "info": {
    "version": "2016-08-05T01:18:49Z",
    "title": "{{ api.title }}"
  },
  "host": "{{ api.host }}",
  "title": "{{ api.title }}",
  "basePath": "/{{ api.version }}",
  "schemes": [
    "https"
  ],
  "paths": {
    {{ paths | join: "," }}
  },
  "securityDefinitions": {
    "api_key": {
      "type": "apiKey",
      "name": "x-api-key",
      "in": "header"
    },
    "sigv4": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header",
      "x-amazon-apigateway-authtype": "awsSigv4"
    }
  },
  "definitions": {
    {{ definitions | join: "    ," }}
  }
}
