{
  "post": {
    "tags": [
      "{{ parent }}"
    ],
    {{consume_json}},
    {{produce_json}},
    "parameters": [
      {
        "in": "body",
        "name": "CreateField",
        "required": true,
        "schema": {
          "$ref": "#/definitions/CreateField"
        }
      }
    ],
    "responses": {
      {{Success_200}},
      {{std_error_responses}}
    },
    {{security_key_iam}},
    "x-amazon-apigateway-integration": {
      {{inject_org_id_as_template}},
      "credentials": "arn:aws:iam::766732214526:role/apigateway_schema",
      "uri": "arn:aws:apigateway:us-east-1:lambda:path/2015-03-31/functions/arn:aws:lambda:us-east-1:766732214526:function:{{lambda.SchemaFieldCreate.function.name}}/invocations",
      "passthroughBehavior": "never",
      "responses": {
        "default": {
          "statusCode": "200"
        },
        {{std_error_mappings}}
      },
      "httpMethod": "POST",
      "type": "aws"
    }
  },
  "get": {
    "tags": [
      "{{ parent }}"
    ],
    {{consume_json}},
    {{produce_json}},
    "parameters": [
      {
        "name": "metricName",
        "in": "query",
        "required": false,
        "type": "string"
      },
      {
      "name": "fieldName",
      "in": "query",
      "required": false,
      "type": "string"
      }
    ],
    "responses": {
      {{Success_200}},
      {{std_error_responses}}
    },
    {{security_key_iam}},
    "x-amazon-apigateway-integration": {
      "credentials": "arn:aws:iam::766732214526:role/apigateway_schema",
      "requestTemplates": {
        "application/json": "{\n    \"orgId\" : \"$context.identity.apiKey\",\n    \"metricName\": \"$input.params('metricName')\",\n    \"fieldName\" : \"$input.params('fieldName')\"\n}\n"
      },
      "uri": "arn:aws:apigateway:us-east-1:lambda:path/2015-03-31/functions/arn:aws:lambda:us-east-1:766732214526:function:{{lambda.SchemaFieldRead.function.name}}/invocations",
      "passthroughBehavior": "never",
      "responses": {
        "default": {
          "statusCode": "200"
        },
        {{std_error_mappings}}
      },
      "httpMethod": "POST",
      "type": "aws"
    }
  },
  "patch": {
    "tags": [
      "{{ parent }}"
    ],
    {{consume_json}},
    {{produce_json}},
    "parameters": [
      {
        "in": "body",
        "name": "UpdateField",
        "required": true,
        "schema": {
          "$ref": "#/definitions/UpdateField"
        }
      }
    ],
    "responses": {
      {{Success_200}},
      {{std_error_responses}}
    },
    {{security_key_iam}},
    "x-amazon-apigateway-integration": {
      {{inject_org_id_as_template}},
      "credentials": "arn:aws:iam::766732214526:role/apigateway_schema",
      "uri": "arn:aws:apigateway:us-east-1:lambda:path/2015-03-31/functions/arn:aws:lambda:us-east-1:766732214526:function:{{lambda.SchemaFieldUpdate.function.name}}/invocations",
      "passthroughBehavior": "never",
      "responses": {
        "default": {
          "statusCode": "200"
        },
        {{std_error_mappings}}
      },
      "httpMethod": "POST",
      "type": "aws"
    }
  },
  "delete": {
    "tags": [
      "{{ parent }}"
    ],
    {{consume_json}},
    {{produce_json}},
    "parameters": [
      {
        "in": "body",
        "name": "DeleteField",
        "required": true,
        "schema": {
          "$ref": "#/definitions/DeleteField"
        }
      }
    ],
    "responses": {
      {{Success_200}},
      {{std_error_responses}}
    },
    {{security_key_iam}},
    "x-amazon-apigateway-integration": {
      {{inject_org_id_as_template}},
      "credentials": "arn:aws:iam::766732214526:role/apigateway_schema",
      "uri": "arn:aws:apigateway:us-east-1:lambda:path/2015-03-31/functions/arn:aws:lambda:us-east-1:766732214526:function:{{lambda.SchemaFieldDelete.function.name}}/invocations",
      "passthroughBehavior": "never",
      "responses": {
        "default": {
          "statusCode": "200"
        },
        {{std_error_mappings}}
      },
      "httpMethod": "POST",
      "type": "aws"
    }
  }
}
